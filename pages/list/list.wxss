.line_color {
  background-color: rgba(0, 0, 0, 0.5);
}
.wrap {
  width: 100%;
  height: 100%;
}
.wrap .list_content-popup {
  position: fixed;
}
.wrap .list_content {
  width: 100%;
  overflow: hidden;
}
.wrap .list_content .header {
  position: fixed;
  top: 0;
  width: 100%;
  height: 160rpx;
  padding: 30rpx 20rpx 0;
  overflow: hidden;
  box-shadow: 0 0rpx 5rpx 0rpx #aaa;
  background-color: #fff;
}
.wrap .list_content .header .select_item {
  display: flex;
  justify-content: center;
  align-items: center;
}
.wrap .list_content .header .select_item .select_same {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 200rpx;
  height: 72rpx;
  text-align: center;
  font-size: 32rpx;
  color: #999;
}
.wrap .list_content .header .select_item .select_active {
  border-bottom: 4rpx solid var(--themeColor);
  color: var(--themeColor);
}
.wrap .list_content .header .select {
  display: flex;
  height: 70rpx;
}
.wrap .list_content .header .select .select_list {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}
.wrap .list_content .header .select .select_list .select_title {
  font-size: 32rpx;
  color: #333;
}
.wrap .list_content .header .select .select_list .fontColor {
  font-size: 34rpx;
  color: var(--themeColor);
}
.wrap .list_content .header .select .select_list .select_img {
  width: 15rpx;
  height: 8rpx;
  margin-left: 9rpx;
}
.wrap .list_content .policy_list {
  margin-top: 175rpx;
  padding: 0 20rpx;
}
.wrap .list_content .policy_list .policy_item {
  display: flex;
  align-items: center;
  justify-content: space-between;
  width: 100%;
  height: 100%;
  padding: 20rpx 10rpx;
  border-bottom: 1rpx solid var(--baseRgb);
}
.wrap .list_content .policy_list .policy_item .policy_list-img {
  flex: 1;
  height: 130rpx;
  margin-right: 15rpx;
  border-radius: 10rpx;
}
.wrap .list_content .policy_list .policy_item .imgShow {
  display: none;
}
.wrap .list_content .policy_list .policy_item .policy_title-container {
  flex: 3;
  padding-top: 20rpx;
}
.wrap .list_content .policy_list .policy_item .policy_title-container .policy_title {
  line-height: 36rpx;
  margin-bottom: 15rpx;
  color: #333;
  font-family: Source Han Sans CN;
  font-size: 28rpx;
}
.wrap .list_content .policy_list .policy_item .policy_title-container .policy_other {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.wrap .list_content .policy_list .policy_item .policy_title-container .policy_other .policy_title-introduce {
  display: flex;
}
.wrap .list_content .policy_list .policy_item .policy_title-container .policy_other .policy_title-introduce view {
  display: inline-block;
  line-height: 30rpx;
  margin-right: 10rpx;
  border-radius: 5rpx;
  padding: 0 10rpx;
  font-size: 24rpx;
  color: #fff;
}
.wrap .list_content .policy_list .policy_item .policy_title-container .policy_other .policy_title-introduce .policy_from {
  background-color: var(--themeColor);
}
.wrap .list_content .policy_list .policy_item .policy_title-container .policy_other .policy_title-introduce .policy_attribute-one {
  background-color: #34c1e9;
}
.wrap .list_content .policy_list .policy_item .policy_title-container .policy_other .policy_title-introduce .policy_attribute-two {
  background: #f7a939;
}
.wrap .list_content .policy_list .policy_item .policy_title-container .policy_other .policy_title-time {
  color: #999;
}
.wrap .popup_container {
  position: fixed;
  top: 165rpx;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.5);
  animation: down 1s;
}
@keyframes down {
  0% {
    background: rgba(0, 0, 0, 0);
  }
  100% {
    background: rgba(0, 0, 0, 0.5);
  }
}
.wrap .popup_container .popup_content-wrap {
  width: 100%;
  padding: 0 25rpx;
  border-radius: 0 0 30rpx 30rpx;
  background-color: #fff;
}
.wrap .popup_container .popup_content-wrap .popup_content {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding-top: 20rpx;
  box-sizing: border-box;
}
.wrap .popup_container .popup_content-wrap .popup_content .policy_themes {
  width: 160rpx;
  line-height: 50rpx;
  margin-top: 25rpx;
  text-align: center;
  border-radius: 6rpx;
  background-color: #f6f6f6;
  color: #666;
}
.wrap .popup_container .popup_content-wrap .popup_content .policy_themes text {
  font-size: 28rpx;
  letter-spacing: 2rpx;
}
.wrap .popup_container .popup_content-wrap .popup_content .active_themes {
  background-color: var(--themeColor);
  color: #fff;
}
.wrap .popup_container .popup_content-wrap .popup_content .seat_view {
  width: 160rpx;
  height: 0;
  border: none;
  padding: 0;
}
.wrap .popup_container .popup_content-wrap .popup_content-choose {
  display: flex;
  justify-content: center;
  padding: 60rpx 0;
}
.wrap .popup_container .popup_content-wrap .popup_content-choose .popup_cancel {
  width: 140rpx;
  line-height: 60rpx;
  text-align: center;
  border-radius: 20rpx;
  font-size: 30rpx;
  color: #999;
  background-color: #f6f6f6;
}
.wrap .popup_container .popup_content-wrap .popup_content-choose .popup_true {
  width: 360rpx;
  line-height: 60rpx;
  margin-left: 40rpx;
  text-align: center;
  border-radius: 20rpx;
  font-size: 40rpx;
  color: #fff;
  background-color: var(--themeColor);
}
.wrap .popup_display {
  display: none;
}
.wrap .popup_show {
  background: rgba(0, 0, 0, 0.5);
}
