				<navigator class="enterprise_list-nav" url="../list/list?id=1"  hover-class="none">
					<image src="../../images/policy_08.png" lazy-load="false" />
				</navigator>
			</view>
		</view>
		<!-- 惠企政策结束 -->
		<!-- 民生政策开始 -->
		<view class="enterprise_policy">
			<view class="enterprise_title">
				<view class="enterprise_policy-name">
					<view class="enterprise_policy-line"></view>
					<text>民生政策</text>
				</view>
				<navigator class="enterprise_policy-more" url="../list/list?id=2"  hover-class="none">
